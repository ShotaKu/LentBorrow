<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lentborrow.cs3231.com.lentborrow">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".RequestBoxActivity" />
        <activity android:name=".RequestDetailActivity" />
        <activity android:name=".BookDetailActivity" />
        <activity android:name=".UserDetailActivity" />
        <activity android:name=".TradeScheduleActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".UserBookActivity" />
        <activity android:name=".AddBookActivity"></activity>
    </application>

</manifest>